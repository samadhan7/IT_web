						  <button class="btn" type="submit"><i class="fa fa-envelope-o"> </i></button>
						</span>
					  </div>
					</form> -->
				</div>
				<!--/ end about us -->

			</div><!-- Row end -->
		</div><!-- Container end -->
	</footer><!-- Footer end -->
